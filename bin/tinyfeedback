#!/usr/bin/env python

import os
import sys
sys.path.insert(0, os.path.join(os.path.dirname(__file__), '..'))

import tinyfeedback.webserver
import tinyfeedback.helper


LOG_PATH = ''
#LOG_PATH = '/var/log/tinyfeedback.log'
LOG_LEVEL = 'DEBUG'


if __name__ == '__main__':
    tinyfeedback.webserver.set_up_server(host=tinyfeedback.helper.HOST, port=tinyfeedback.helper.PORT,
            log_path=LOG_PATH, log_level=LOG_LEVEL)
